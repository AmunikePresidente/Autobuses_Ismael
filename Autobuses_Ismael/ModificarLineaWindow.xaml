<Window x:Class="Autobuses_Ismael.ModificarLineaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Autobuses_Ismael"
        mc:Ignorable="d"
        Title="ModificarLineaWindow" Height="450" Width="800">
    <Grid>
        <local:UserControlItinerario HorizontalAlignment="Center" VerticalAlignment="Center" Width="800" RenderTransformOrigin="0.5,0.5" Height="435">
            <local:UserControlItinerario.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-0.554"/>
                    <RotateTransform/>
                    <TranslateTransform X="-1.853"/>
                </TransformGroup>
            </local:UserControlItinerario.RenderTransform>
        </local:UserControlItinerario>
    </Grid>
</Window>
