<UserControl x:Class="Autobuses_Ismael.ItiControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Autobuses_Ismael"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ComboBox x:Name="cbOriginal" HorizontalAlignment="Center" Margin="0,161,0,0" VerticalAlignment="Top" Width="348" SelectionChanged="ComboBox_SelectionChanged" Height="31"/>
        <Label Content="Seleccione el municipio a modificar" HorizontalAlignment="Center" Margin="0,130,0,0" VerticalAlignment="Top" Width="218"/>
        <TextBox x:Name="intervalotxt" HorizontalAlignment="Center" Margin="0,288,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="348" Height="29"/>
        <Label Content="Modifique el intervalo " HorizontalAlignment="Center" Margin="0,257,0,0" VerticalAlignment="Top" Width="132"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,363,0,0" VerticalAlignment="Top" Width="132" Height="31" Click="Button_Click"/>
    </Grid>
</UserControl>
